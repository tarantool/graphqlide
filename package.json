{
  "name": "graphqlide",
  "version": "scm-1",
  "private": true,
  "homepage": "https://github.com/tarantool/graphqlide",
  "dependencies": {
    "@emotion/css": "11.7.1",
    "@tarantool.io/frontend-core": "8.0.2",
    "@tarantool.io/ui-kit": "0.50.5",
    "antd": "4.18.3",
    "graphiql": "1.5.16",
    "graphql": "16.2.0",
    "prettier": "2.5.1",
    "react-hot-keys": "2.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/eslint-parser": "7.16.5",
    "@babel/preset-flow": "7.16.7",
    "@tarantool.io/lua-bundler-webpack-plugin": "1.0.1",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "autoprefixer": "10.4.2",
    "babel-jest": "27.4.6",
    "babel-loader": "8.2.3",
    "babel-preset-react-app": "10.0.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "4.1.2",
    "css-loader": "6.5.1",
    "dotenv": "11.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-webpack-plugin": "3.1.1",
    "file-loader": "6.2.0",
    "file-saver": "2.0.5",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.4.7",
    "less-loader": "10.2.0",
    "mime-types": "2.1.34",
    "object-assign": "4.1.1",
    "path-browserify": "^1.0.1",
    "postcss": "8.4.5",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "6.2.1",
    "process": "^0.11.10",
    "promise": "8.1.0",
    "raf": "3.4.1",
    "react-dev-utils": "12.0.0",
    "source-map-explorer": "2.5.2",
    "string-replace-loader": "3.1.0",
    "style-loader": "3.3.1",
    "svg-sprite-loader": "6.0.11",
    "svgo": "2.8.0",
    "svgo-loader": "3.0.0",
    "terser-webpack-plugin": "5.3.0",
    "typescript": "^4.5.4",
    "url": "^0.11.0",
    "url-loader": "4.1.1",
    "webpack": "5.66.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-dev-server": "4.7.3",
    "webpack-manifest-plugin": "4.1.1",
    "whatwg-fetch": "3.6.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "analyze": "source-map-explorer build/static/graphqlide/main.*",
    "test": "node scripts/test.js test --env=jsdom --watch",
    "test_once": "node ./scripts/test.js test --env=jsdom",
    "lint": "NODE_ENV=production eslint --ext .ts,.tsx ./src",
    "lint-fix": "NODE_ENV=production eslint --ext .ts,.tsx --fix ./src",
    "build-rock": "make rock"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "resolutions": {
    "**/@babel/runtime": "7.16.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-flow",
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ],
    "plugins": [
      "@emotion"
    ]
  }
}
