    "less-loader": "^7.0.1",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "normalize-url": "^5.3.1",
    "object-assign": "4.1.1",
    "postcss": "^8.3.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "4.3.0",
