    "@emotion/css": "11.5.0",
    "graphiql": "1.5.1",
    "graphql": "15.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-decorators": "7.16.0",
    "@babel/preset-flow": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@babel/runtime": "7.16.3",
    "@babel/traverse": "7.16.3",
    "@babel/types": "7.16.0",
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/eslint-plugin": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@emotion/styled-base": "11.0.0",
    "@prettier/plugin-xml": "1.1.0",
    "@tarantool.io/lua-bundler-webpack-plugin": "1.0.1",
    "array-move": "4.0.0",
    "autoprefixer": "10.4.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "babel-preset-react-app": "10.0.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "4.1.2",
    "css-loader": "6.5.1",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-emotion": "11.0.0",
    "eslint-plugin-flowtype": "5.10.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-relay": "1.8.3",
    "file-loader": "6.2.0",
    "file-saver": "2.0.5",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.3.1",
    "less-loader": "10.2.0",
    "lodash": "4.17.21",
    "mime-types": "2.1.34",
    "normalize-url": "7.0.2",
    "object-assign": "4.1.1",
    "postcss": "8.3.11",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "6.2.0",
    "prettier": "2.4.1",
    "promise": "8.1.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-dev-utils": "12.0.0-next.47",
    "resolve": "1.20.0",
    "source-map-explorer": "2.5.2",
    "string-replace-loader": "3.0.3",
    "style-loader": "3.3.1",
    "svg-sprite-loader": "6.0.11",
    "svgo": "2.8.0",
    "svgo-loader": "3.0.0",
    "terser-webpack-plugin": "5.2.5",
    "touch": "3.1.0",
    "url-loader": "4.1.1",
    "webpack": "5.64.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-dev-server": "4.4.0",
    "webpack-manifest-plugin": "4.0.2",
    "whatwg-fetch": "3.6.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "analyze": "source-map-explorer build/static/graphqlide/main.*",
    "test": "node scripts/test.js test --env=jsdom --watch",
    "test_once": "node ./scripts/test.js test --env=jsdom",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "build-rock": "make rock"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "resolutions": {
    "**/@babel/runtime": "7.16.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-flow",
      ["react-app", { "absoluteRuntime": false }]
    ],
    "plugins": [
      "@emotion"
    ]
  }
}
